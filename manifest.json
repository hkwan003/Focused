{
    "name": "Focused",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Prohibits certain social media sites on your browser for when user requires absolute focus.",
    "browser_action": {
        "default_icon": "focused.png",
        "default_popup": "popup.html"},

        "background": {
            "scripts": ["background.js"]
        }

    "permissions" :[
    "tabs"
    ]

}
